@using SEO_Crowler.Web.Helper;
@model SEO_Crowler.Web.Areas.Dashboard.ViewModel.SearchViewModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers


<div class="pl-lg-4">
    <div class="form-group row">
        <div class="col-sm-6 mb-3 mb-sm-0">
            <div class="form-group">
                <label class="form-control-label" for="SearchPhrase">Search Phrase<span class="text-danger">*</span></label>
                <input class="form-control" type="text" asp-for="@Model.SearchPhrase" placeholder="Search keyword" />
            </div>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-6">
            <div class="form-group">
                <label class="form-control-label" for="URL">URL<span class="text-danger">*</span></label>
                <input class="form-control" type="text" asp-for="@Model.URL" placeholder="Enter URL" />
            </div>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-6">
            <button type="submit" class="class=" btn btn-primary btn-user btn-block"" id="SearchButton">Search</button>
        </div>
    </div>
      
    </div>


